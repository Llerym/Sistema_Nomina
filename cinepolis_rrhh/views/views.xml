<odoo>
    <record id="view_employee_form_branch_cinepolis" model="ir.ui.view">
        <field name="name">hr.employee.form.branch</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='gender']" position="after">
                <field name="has_capacity"/>
            </xpath>
        </field>
    </record>
</odoo>
